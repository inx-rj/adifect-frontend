{
    "version": 2,
    "builds": [
        {
            "src": "server/**",
            "use": "@vercel/node"
        },
        {
            "src": "src/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server/$1"
        },
        {
            "src": "/(.*)",
            "dest": "src/$1"
        }
    ]
}